<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<%@ include file="../inc/top.jsp"%>
</head>
<body>

	<div class="container-fluid">
		<div class="row">
			<div class="col-md-2 sidebar">
				<ul class="nav nav-sidebar" style="width: 80px">
					<li><a href="#">회원목록</a></li>
					<li><a href="#">영화목록</a></li>
				</ul>
			</div>
			<div class="col-md-10 col-md-offset-2 main">
				<h1 class="page-header">매니저 페이지</h1>

				<div class="row placeholders">
					<div class="col-xs-6 col-sm-3 placeholder">
						<img data-src="holder.js/200x200/auto/sky" class="img-responsive"
							alt="200x200"
							src="data:image/svg+xml;base64,"
							data-holder-rendered="true">
						<h4>방문자 수</h4>
						<span class="text-muted">Something else</span>
					</div>
					<div class="col-xs-6 col-sm-3 placeholder">
						<img data-src="holder.js/200x200/auto/vine" class="img-responsive"
							alt="200x200"
							src="data:image/svg+xml;base64,"
							data-holder-rendered="true">
						<h4>Label</h4>
						<span class="text-muted">Something else</span>
					</div>
					<div class="col-xs-6 col-sm-3 placeholder">
						<img data-src="holder.js/200x200/auto/sky" class="img-responsive"
							alt="200x200"
							src="data:image/svg+xml;base64,"
							data-holder-rendered="true">
						<h4>Label</h4>
						<span class="text-muted">Something else</span>
					</div>
					<div class="col-xs-6 col-sm-3 placeholder">
						<img data-src="holder.js/200x200/auto/vine" class="img-responsive"
							alt="200x200"
							src="data:image/svg+xml;base64,"
							data-holder-rendered="true">
						<h4>Label</h4>
						<span class="text-muted">Something else</span>
					</div>
				</div>
			
			<!-- 새로 가입한 사람 -->
			<div class="row">
				<h3>새로운가입자</h3>
				<div class="col-md-6">
				</div>
				<div class="col-md-6">
					<table class="table table-hover table-sm">
						<thead>
							<tr>
								<th>
									#
								</th>
								<th>
									Product
								</th>
								<th>
									Payment Taken
								</th>
								<th>
									Status
								</th>
							</tr>
						</thead>
						<tbody>
							<tr>
								<td>
									1
								</td>
								<td>
									TB - Monthly
								</td>
								<td>
									01/04/2012
								</td>
								<td>
									Default
								</td>
							</tr>
						</tbody>
					</table>
				</div>
			</div>
				
			<!-- 회원테이블 -->
				<h2 class="sub-header">회원테이블</h2>
				<div class="table-responsive">
					<table class="table table-striped">
						<thead>
							<tr>
								<th>회원번호</th>
								<th>아이디</th>
								<th>이름</th>
								<th>가입일</th>
								<th>탈퇴일</th>
							</tr>
						</thead>
						<tbody>
						<c:forEach var="mngVo" items="${mnglist }">
							<tr>
								<td>${mngVo.memNo }</td>
								<td>
									<a href="#">${mngVo.id }</a>
								</td>
								<td>${mngVo.name }</td>
								<td>${mngVo.regdate }</td>
								<td>${mngVo.outdate }</td>
							</tr>
						</c:forEach>
						</tbody>
					</table>
				</div>
				
				<!-- 버튼 -->
				<div class="buttonwarp">
					<input type="button" name="admin" value="등록">
					<input type="button" name="edit" value="수정">
					<input type="button" name="dele" value="삭제">
				</div>
			</div>
		</div>
		
		
	</div>

</body>
</html>